        scroll-snap-type: x mandatory;
    }
    .gallery-item {
        flex: 0 0 auto;
        scroll-snap-align: center;
        width: 300px;
        height: 200px;
        border-radius: 8px;
        box-shadow: 0 4px 8px rgba(0,0,0,0.1);
        background-color: #fff;
    
    }
    .gallery-item img {
        width: 100%;
        height: 100%;
        object-fit: cover;
        display: block;
    }    

    .prev-btn, .next-btn {
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        background: rgba(0,0,0,0.5);
        border: none;
        color: #fff;
        width: 40px;
        height: 40px;
        cursor: pointer;
        font-size: 20px;
        border-radius: 50%;
        z-index: 10;
    }

    .prev-btn { left: 10px; }
    .next-btn { right: 10px; }

